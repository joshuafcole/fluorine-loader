    "build": "tsc",
    "dev": "tsc -w",
    "clean": "rm *.js; rm *.js.map; rm *.d.ts; rm *.tgz;",
    "prepack": "npm run clean; npm run build"
  },
  "author": "Joshua Cole",
  "license": "MIT",
  "dependencies": {
