    "acorn-walk": "^6.1.1",
    "magic-string": "https://github.com/joshuafcole/magic-string/releases/download/v0.25.3-include-sourcemaps/magic-string-0.25.3-include-sourcemaps.tgz",
    "md5-jkmyers": "0.0.1",
    "loader-utils": "^1.2.3",
    "source-map": "^0.7.3"
